let respostaUm = prompt ("Você está usando preto?")
let respostaDois = prompt ("Você está usando amarelo?")
let respostaTres = prompt ("Você está usando vermelho?")

console.log ("Você está usando preto?" , respostaUm)
console.log ("Você está usando amarelo?" , respostaDois)
console.log ("Você está usando vermelho?" , respostaTres)
